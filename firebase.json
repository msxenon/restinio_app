{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "restinio-app",
          "appId": "1:73363374617:android:2a5dbe522f70ded1c8a088",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "restinio-app",
          "appId": "1:73363374617:ios:7545dea9f468e547c8a088",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "restinio-app",
          "configurations": {
            "android": "1:73363374617:android:2a5dbe522f70ded1c8a088",
            "ios": "1:73363374617:ios:7545dea9f468e547c8a088",
            "web": "1:73363374617:web:cf0af327b2bbfb9ec8a088"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
